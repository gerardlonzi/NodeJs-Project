<%- include('partiel/head', { title: "cours" }) %>
<meta name="description" content="   ">
</head>

<body>
    <%- include('partiel/header') %>
    <main id="content-page">
        <section class="barner-short">
            <div class="container">
                    <h1>Nos Professeurs
                    </h1>
            </div>
            <img src="images/slider-bottom-pattern.png" alt="pattern">
        </section>
        <div >
            <div class="container">
                <ul class="allcourse" style="list-style: none;margin-top: 50px;">
                    <% if (users !=='undefined' && users!==null && users.length>0) { %>
                        <% users.forEach(element => { %>
                            <li class="" style="box-shadow: 0px 0px 10px rgb(226, 226, 226);
">
                               <div class="splite-image">
                                   <img src="<%= element.profilePicture || "" %>" alt="<%= element.name %>">
                                   <div>
                                    <div>
                                        <a href=""><i class="bi bi-facebook"></i></a>
                                        <a href=""><i class="bi bi-twitter-x"></i></a>
                                        <a href=""><i class="bi bi-linkedin"></i></a>
                                        <a href=""><i class="bi bi-youtube"></i></a>
                                    </div>
                                   </div>
                               </div>
                               <div class="cell-right">
                                   <div class="testimonial-name"><%= element.name %>
                                   </div>
                                   <div class="testimonial-job"><%= element.poste %>
                                   </div>
                               </div>
                           </li>
                        <% }) %>
                       <% } %>
                       
                </ul>
            </div>
            
        </div>
    </main>
    

</body>
</html>



